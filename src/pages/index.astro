---
import { render } from "astro:content";
import { getEntry } from "astro:content";
import CustomLayout from "../layouts/CustomLayout.astro";
import { extendHtml } from "nebula-atoms/extendHtml";

const page = await getEntry("pages", "199bfa08-e7ee-814e-90fa-f0d8beb63dbc")!;
if (page.rendered?.html) page.rendered.html = extendHtml(page.rendered.html);
const { Content } = await render(page);
---

<CustomLayout title={"ZOAL ENCRE"}>
  <Content />
</CustomLayout>
